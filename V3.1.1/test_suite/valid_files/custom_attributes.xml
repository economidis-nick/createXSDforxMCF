						<int key="empty_integer"></int>
						<int key="empty_integer_with_spaces">   </int>
						<string key="some_string">Steel_225_ISO</string>
						<real key="float_with_dot"> 223 </real>
						<real key="float_ending_with_dot"> 223. </real>
						<real key="float_ending_with_number"> 223.3 </real>
						<!-- <real key="only_dot"> . </real> -->
						<!-- <real key="sign_and_dot"> -. </real> -->
						<real key="dot_first"> .3 </real>
						<real key="positive_dot_first"> +3. </real>
						<real key="negative_dot_first"> -.1 </real>
						<real key="with_positive_exp"> .1e+3</real>
						<real key="with_positive_Exp"> .1E+3</real>
						<real key="with_positive_Exp2"> .1E+003</real>
						<real key="leading_zeroes_with_positive_exp"> 000.1E+003</real>
